############################
#        COMPOSE           #
############################
COMPOSE_PROJECT_NAME=batch-queries-postgresql

############################
#        IMAGE             #
############################
POSTGRESQL_IMAGE_NAME=bitnami/postgresql
POSTGRESQL_IMAGE_VERSION=17

############################
#        ACCESS            #
############################
POSTGRESQL_USER_NAME=user
POSTGRESQL_USER_PASSWORD=password
POSTGRESQL_POSTGRES_PASSWORD=admin_password

############################
#        DATABASE          #
############################
POSTGRESQL_DATABASE_NAME=database

############################
#        RESOURCES         #
############################
POSTGRESQL_SHARED_BUFFERS_SIZE=256m
POSTGRESQL_TOTAL_MEMORY_SIZE=512m
POSTGRESQL_CPU_COUNT=0.5

############################
#        NETWORK           #
############################
POSTGRESQL_INTERNAL_PORT=5432
POSTGRESQL_EXPOSED_PORT=5432